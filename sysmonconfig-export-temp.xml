			<ImageLoaded name="T1073,DLL_Side-Loading" condition="contains any">admin$;c$;\\;\appdata\;\temp\</ImageLoaded>
			<ImageLoaded condition="begin with" name="T1073,DLL_Side-Loading">c:\programdata\</ImageLoaded>
